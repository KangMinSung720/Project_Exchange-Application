<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.aiw">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".Activity.SignInActivity"
            android:windowSoftInputMode="adjustResize">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.SignUpActivity" />
        <activity android:name=".Activity.MainActivity" />
        <activity android:name=".Activity.CurrencyActivity" />
        <activity android:name=".Activity.BaseActivity" />
        <activity android:name=".Activity.ExchangeOneActivity" />
        <activity android:name=".Activity.ExchangeTwoActivity" />
        <activity android:name=".Activity.ExchangeThreeActivity" />
        <activity android:name=".Activity.AddressActivity" />
        <activity android:name=".Activity.ExchangeFourthActivity" />
        <activity android:name=".Activity.MyMenuActivity"></activity>
    </application>

</manifest>